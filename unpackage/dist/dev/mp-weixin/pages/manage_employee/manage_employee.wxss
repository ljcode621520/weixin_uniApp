.page {
  padding: 35rpx;
}
.list .item {
  padding: 30rpx 15rpx;
  border-bottom: solid 1rpx #55aaff;
  font-size: 30rpx;
  color: #333;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.list .item .value {
  color: #3474FF;
}
.list .item .icon {
  width: 40rpx;
}
